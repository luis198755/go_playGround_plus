# Configuración del servidor
SERVER_PORT=8080
SERVER_HOST=0.0.0.0
DEBUG_MODE=false
STATIC_FILES_DIR=/app/build

# Límites y seguridad
MAX_REQUESTS_PER_MINUTE=30
MAX_CODE_LENGTH=10000
MAX_OUTPUT_LENGTH=10000
EXECUTION_TIMEOUT_SECONDS=10
ALLOWED_ORIGINS=*

# Ejecución de código Go
GO_EXECUTABLE_PATH=/usr/local/go/bin/go
TEMP_DIR=/tmp/go-playground
CLEANUP_INTERVAL_MINUTES=60

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
